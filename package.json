{
  "name": "classfunc-helpers",
  "version": "2.0.16",
  "description": "ClassFunc Helpers",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "node esbuild.cjs",
    "build:ts": "tsc",
    "build:commit": "yarn run build:ts && yarn generate-docs && git add --all && git commit -m 'build'",
    "generate-docs": "typedoc --plugin typedoc-plugin-markdown --readme ./README.md --out __docs__ index.ts"
  },
  "files": [
    "index.ts",
    "index.js",
    "index.d.ts",
    "db",
    "diff",
    "env",
    "json"
  ],
  "exports": {
    ".": {
      "require": "./index.js",
      "default": "./index.js",
      "types": "./index.d.ts"
    },
    "./json": {
      "require": "./json/index.js",
      "default": "./json/index.js",
      "types": "./json/index.d.js"
    },
    "./env": {
      "require": "./json/index.js",
      "default": "./json/index.js",
      "types": "./json/index.d.js"
    },
    "./diff": {
      "require": "./json/index.js",
      "default": "./json/index.js",
      "types": "./json/index.d.js"
    },
    "./db": {
      "require": "./json/index.js",
      "default": "./json/index.js",
      "types": "./json/index.d.js"
    },
    "./package.json": "./package.json",
    "./index.d.ts": "./index.d.ts"
  },
  "keywords": [],
  "author": "ClassFunc Softwares JSC",
  "repository": {
    "url": "https://github.com/ClassFunc/classfunc-helpers.git"
  },
  "license": "ISC",
  "dependencies": {
    "deep-diff": "^1.0.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "esbuild": "^0.14.29",
    "@types/lodash": "^4.14.180",
    "esbuild-plugin-d.ts": "^1.1.0",
    "glob": "^7.2.0",
    "typedoc": "^0.22.13",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript": "^4.6.3"
  }
}
