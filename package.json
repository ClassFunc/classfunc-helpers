{
  "name": "classfunc-helpers",
  "version": "2.0.7",
  "description": "ClassFunc Helpers",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:esm": "tsc --module es2015 --target es5 --outDir __dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir __dist/cjs",
    "build:commit": "yarn run build && yarn generate-docs && git add --all && git commit -m 'build'",
    "generate-docs": "typedoc --plugin typedoc-plugin-markdown --readme ./README.md --out __docs__ index.ts"
  },
  "exports": {
    ".": "./index.js",
    "./json": "./json/index.js",
    "./env": "./json/env.js",
    "./diff": "./json/diff.js",
    "./db": "./json/db.js",
    "./package.json": "./package.json",
    "./index.d.ts": "./index.d.ts"
  },
  "keywords": [],
  "author": "ClassFunc Softwares JSC",
  "repository": {
    "url": "https://github.com/ClassFunc/classfunc-helpers.git"
  },
  "license": "ISC",
  "dependencies": {
    "deep-diff": "^1.0.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.180",
    "typedoc": "^0.22.13",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript": "^4.6.3"
  }
}
